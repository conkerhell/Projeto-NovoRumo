@model NovoRumoProjeto.Areas.Admin.Models.ContactViewModel

<div class="cell">
    <div class="grid-x title-bar-admin">
        <div class="small-6">
            <h1>Contato</h1>
        </div>
        <div class="small-6">
            <div class="float-right">
                @Html.EditButton("Contact", 1)
            </div>
        </div>
    </div>
</div>

<div class="grid-x cell">
    <div class="cell small-2">
        @Html.LabelFor(model => model.Email)
        @Html.LabelFor(model => model.Telephone)
        @Html.LabelFor(model => model.Mobile)
        @Html.LabelFor(model => model.SecondaryMobile)
        @Html.LabelFor(model => model.CEP)
        @Html.LabelFor(model => model.Address)
    </div>

    <div class="small-10">
        <div class="small-12">
            @Html.DisplayFor(model => model.Email)
        </div>
        <div class="small-12">
            @Html.DisplayFor(model => model.Telephone)
        </div>
        <div class="small-12">
            @Html.DisplayFor(model => model.Mobile)
        </div>
        <div class="small-12">
            @Html.DisplayFor(model => model.SecondaryMobile)
        </div>
        <div class="small-12">
            @Html.DisplayFor(model => model.CEP)
        </div>
        <div class="small-12">
            @Html.Raw(Model.Address)
        </div>
    </div>

</div>

