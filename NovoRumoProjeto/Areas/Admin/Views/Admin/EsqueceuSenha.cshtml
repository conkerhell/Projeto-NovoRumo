@model NovoRumoProjeto.Areas.Admin.Models.ForgotPasswordViewModel

@using (Html.BeginForm("EsqueceuSenha", "Admin", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.LabelFor(m => m.Email)
    @Html.TextBoxFor(m => m.Email)
    @Html.ValidationMessageFor(m => m.Email)
    <input type="submit" class="button primary" value="Enviar Senha" />
}
